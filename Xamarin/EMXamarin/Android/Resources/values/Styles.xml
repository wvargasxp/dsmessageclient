<?xml version="1.0" encoding="UTF-8" ?>
<resources>
  	<style name="Theme.Splash" parent="android:Theme">
    	<item name="android:windowBackground">@drawable/lander</item>
    	<item name="android:windowNoTitle">true</item>
  	</style>
  	<style name="AppTheme.Light" parent="style/Theme.AppCompat.Light">
  	</style>
  	<style name="AppTheme.Dark" parent="@style/Theme.AppCompat">
  		<item name="colorControlNormal">@color/off_black</item>
  	</style>
  	<style name="BackArrowStyle">
  		<item name="android:background">@drawable/backbuttonstatelistdrawable</item>
  		<item name="android:layout_width">29dp</item>
  		<item name="android:layout_height">29dp</item>
    	<item name="android:layout_marginLeft">5dp</item>
    	<item name="android:layout_alignParentLeft">true</item>
    	<item name="android:layout_centerVertical">true</item>
	</style>
	<style name="CountryImageStyle">
		<item name="android:background">@drawable/us</item>
  		<item name="android:layout_width">66dp</item>
  		<item name="android:layout_height">50dp</item>
    	<item name="android:layout_alignParentLeft">true</item>
    	<item name="android:layout_centerVertical">true</item>
	</style>
	<color name="off_black">#282828</color>
	<color name="off_white">#fafafa</color>
	<color name="off_gray">#ededed</color>
	<color name="off_red">#ff4444</color>
	<color name="em_purple">#632F6D</color>
	<color name="em_blue">#1381D0</color>
	<color name="em_orange">#FC9938</color>
	<color name="em_pink">#BD187B</color>
	<color name="em_green">#23C303</color>
	<color name="divider">#999999</color>
	<color name="dark_gray">#b2b2b2</color>
</resources>